<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="SMURF_Ava.Views.TestWindow"
        Title="TestWindow">
    <Grid ColumnDefinitions="1* 6*" >
        <Border Grid.ColumnSpan="2" BorderBrush="DarkGray" BorderThickness="1" />
                
        <TextBlock Grid.Column="0" Text="[TimeStamp]" Margin="7" Foreground="{Binding }"/>
                
        <Border Grid.Column="0" BorderThickness="0 0 1 0" BorderBrush="DarkGray" />
                
        <TextBlock Grid.Column="1" Text="[Log Details]" Margin="7" Foreground="{Binding }"/>
                
    </Grid>
</Window>
